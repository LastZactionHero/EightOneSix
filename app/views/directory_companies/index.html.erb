<h1>Listing directory_companies</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Address</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>Phone</th>
    <th>Maincontact</th>
    <th>Url</th>
    <th>Notes</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @directory_companies.each do |directory_company| %>
  <tr>
    <td><%= directory_company.name %></td>
    <td><%= directory_company.description %></td>
    <td><%= directory_company.address %></td>
    <td><%= directory_company.city %></td>
    <td><%= directory_company.state %></td>
    <td><%= directory_company.zip %></td>
    <td><%= directory_company.phone %></td>
    <td><%= directory_company.maincontact %></td>
    <td><%= directory_company.url %></td>
    <td><%= directory_company.notes %></td>
    <td><%= link_to 'Show', directory_company %></td>
    <td><%= link_to 'Edit', edit_directory_company_path(directory_company) %></td>
    <td><%= link_to 'Destroy', directory_company, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

